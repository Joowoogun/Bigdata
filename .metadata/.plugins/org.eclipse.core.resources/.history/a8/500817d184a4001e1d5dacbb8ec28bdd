package 변수및자료형;

import java.util.Scanner;

public class Ex01입출력 {

	public static void main(String[] args) {
		//주석 = Comment : 코드에 영향을 끼치지 않는 메모
		//단축키 
		// 1) 글자 크기 조절 : Ctrl + -
		// 2) 한줄 삭제 : Ctrl D
		// 3) 다중 Import : Ctrl Shift O
  		// 4) Run : Ctrl F11
		// 5) 출력문 : syso 에 Ctrl Spacebar
		
		
		// 1. 출력문 
		// ----> () 안쪽에 작성 문자는 " " 추가 
		System.out.println("Hello world!!");
		System.out.println("주우건");
		System.out.println(1234);
		
		// 2. 입력문 
		// 2-1) 입력받는 도구 꺼내오기
		Scanner sc = new Scanner(System.in);
		// 2-2) 입력받기
		
		
		
	}

}
